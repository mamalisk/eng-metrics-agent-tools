# Jira (REST API v2)
# Example: https://mycompany.atlassian.net
JIRA_BASE_URL=
# Bearer token for Authorization header
JIRA_BEARER_TOKEN=
# Optional defaults for convenience
JIRA_DEFAULT_PROJECT_KEY=
JIRA_DEFAULT_SPRINT_ID=

# GitLab (REST API v4)
# Example: https://gitlab.com or https://gitlab.mycompany.com
GITLAB_BASE_URL=
# Personal Access Token (scopes: read_api for reads, api for comments)
GITLAB_TOKEN=
# Optional default project (numeric ID or path like mygroup/myproject)
GITLAB_DEFAULT_PROJECT=
